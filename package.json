{
  "name": "personal-trainer-planner",
  "author": "James Langridge <l@ngridge.com> (https://github.com/james-langridge)",
  "description": "A client management and scheduling app for personal trainers.",
  "repository": {
    "type": "git",
    "url": "https://github.com/james-langridge/personal-trainer-planner.git"
  },
  "bugs": "https://github.com/james-langridge/personal-trainer-planner/issues",
  "license": "MIT",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node -P tsconfig-seed.json -r tsconfig-paths/register --transpileOnly prisma/seed.ts"
  },
  "scripts": {
    "backup": "./scripts/backup.sh",
    "build": "npm run prisma:gen && next build",
    "contentful-typescript-codegen": "contentful-typescript-codegen --output @types/generated/contentful.d.ts",
    "dev": "npm run prisma:gen && next dev --turbopack",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "next lint --fix",
    "prisma:copy-dev": "node prisma/copyUserToDemo.js",
    "prisma:delete-dev": "node prisma/deleteUserEvents.js",
    "prisma:gen": "dotenv -e .env.local -- npx prisma generate",
    "prisma:migrate-dev": "dotenv -e .env.local -- npx prisma migrate dev",
    "prisma:studio": "dotenv -e .env.local -- npx prisma studio",
    "start": "next start",
    "test": "vitest"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@contentful/rich-text-react-renderer": "^16.0.1",
